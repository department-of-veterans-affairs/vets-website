{% include "src/site/includes/header.html" with drupalTags = true %}
{% include "src/site/includes/alerts.drupal.liquid" %}
{% include "src/site/includes/preview-edit.drupal.liquid" %}
{% include "src/site/includes/breadcrumbs.drupal.liquid" %}
<div id="content" class="interior">
    <main class="va-l-detail-page">
    <div class="usa-grid usa-grid-full">
        {% include 'src/site/navigation/facility_sidebar_nav.drupal.liquid' with sidebarData = facilitySidebar %}
        <div class="usa-width-three-fourths">
            {% include "src/site/components/navigation-sidebar-trigger.html" with menuId = "va-detailpage-sidebar" %}
            <article class="usa-content">
                <h1>Our health services</h1>
                <div class="va-introtext">{{ fieldClinicalHealthServi.processed }}</div>

                <div class="usa-grid usa-grid-full vads-u-margin-top--2 vads-u-margin-bottom--3">
                    <div class="usa-grid usa-grid-full vads-u-margin-y--1p5 vads-u-margin-bottom--4">{% assign basePath = entityUrl.path | regionBasePath %}
                        {% include "src/site/facilities/facilities_health_services_buttons.drupal.liquid" with path = basePath %}</div>
                </div>

                {% if socialProgramsPatientFamilyServices.entities.length || healthWellnessPatientFamilyServices.entities.length || primaryCareHealthServices.entities.length || mentalHealthServices.entities.length || specialtyCareHealthServices.entities.length %}
                    <div class="usa-grid usa-grid-full vads-u-margin-top--1p5">
                        <h3>On this page</h3>
                        <ul>
                            {% if featuredHealthServices.entities.length %}
                                <li><a href="#featured-services">Featured services</a></li>
                            {% endif %}
                            {% if primaryCareHealthServices.entities.length %}
                                <li><a href="#primary-health-services">Primary care (Family medicine, internal medicine)</a></li>
                            {% endif %}
                            {% if extendedCareHealthServices.entities.length %}
                                <li><a href="#extended-care-health-services">Extended care and rehabilitation</a></li>
                            {% endif %}
                            {% if homelessHealthServices.entities.length %}
                                <li><a href="#homeless-health-services">Homeless services</a></li>
                            {% endif %}
                            {% if socialProgramsPatientFamilyServices.entities.length %}
                                <li><a href="#social-patient-family-services">Social programs and services</a></li>
                            {% endif %}
                            {% if genomicMedicineHealthServices.entities.length %}
                                <li><a href="#genomic-medicine-health-services">Genomic medicine/medical genetics (Genetic medicine)</a></li>
                            {% endif %}
                            {% if healthWellnessPatientFamilyServices.entities.length %}
                                <li><a href="#wellness-patient-family-services">Health and wellness</a></li>
                            {% endif %}
                            {% if mentalHealthServices.entities.length %}
                                <li><a href="#mental-health-services">Mental and behavioral health</a></li>
                            {% endif %}
                            {% if specialtyCareHealthServices.entities.length %}
                                <li><a href="#specialty-health-services">Specialty care</a></li>
                            {% endif %}
                            {% if veteranCareHealthServices.entities.length %}
                              <li><a href="#veteran-care-health-services">Services for Veteran care</a></li>
                            {% endif %}
                            {% if otherHealthServices.entities.length %}
                                <li><a href="#other-health-services">Other services</a></li>
                            {% endif %}
                        </ul>
                    </div>
                {% endif %}

                {% if featuredHealthServices.entities.length %}
                    <section class="featured-services" id="featured-services">
                        <h2>Featured Services</h2>
                        {% for featuredService in featuredHealthServices.entities %}
                            {% include "src/site/facilities/health_service.drupal.liquid" with healthService = featuredService %}
                        {% endfor %}
                    </section>
                {% endif %}

                {% if primaryCareHealthServices.entities.length %}
                    <section class="primary-health-services" id="primary-health-services">
                        <h2>Primary care (Family medicine, internal medicine)</h2>
                        {% for primaryService in primaryCareHealthServices.entities %}
                            {% include "src/site/facilities/health_service.drupal.liquid" with healthService = primaryService %}
                        {% endfor %}
                    </section>
                {% endif %}

                {% if extendedCareHealthServices.entities.length %}
                    <section class="extended-care-health-services" id="extended-care-health-services">
                        <h2>Extended care and rehabilitation</h2>
                        {% for service in extendedCareHealthServices.entities %}
                            {% include "src/site/facilities/health_service.drupal.liquid" with healthService = service %}
                        {% endfor %}
                    </section>
                {% endif %}

                {% if homelessHealthServices.entities.length %}
                    <section class="homeless-health-services" id="homeless-health-services">
                        <h2>Homeless services</h2>
                        {% for service in homelessHealthServices.entities %}
                            {% include "src/site/facilities/health_service.drupal.liquid" with healthService = service %}
                        {% endfor %}
                    </section>
                {% endif %}

                {% if socialProgramsPatientFamilyServices.entities.length %}
                    <section class="social-patient-family-services" id="social-patient-family-services">
                        <h2>Social programs and services</h2>
                        {% for socialService in socialProgramsPatientFamilyServices.entities %}
                            {% include "src/site/facilities/health_service.drupal.liquid" with healthService = socialService %}
                        {% endfor %}
                    </section>
                {% endif %}

                {% if genomicMedicineHealthServices.entities.length %}
                    <section class="genomic-medicine-health-services" id="genomic-medicine-health-services">
                        <h2>Genomic medicine/medical genetics (Genetic medicine)s</h2>
                        {% for service in genomicMedicineHealthServices.entities %}
                            {% include "src/site/facilities/health_service.drupal.liquid" with healthService = service %}
                        {% endfor %}
                    </section>
                {% endif %}

                {% if healthWellnessPatientFamilyServices.entities.length %}
                    <section class="wellness-patient-family-services" id="wellness-patient-family-services">
                        <h2>Health and wellness</h2>
                        {% for wellnessService in healthWellnessPatientFamilyServices.entities %}
                          {% include "src/site/facilities/health_service.drupal.liquid" with healthService = wellnessService %}
                        {% endfor %}
                    </section>
                {% endif %}

                {% if mentalHealthServices.entities.length %}
                    <section class="mental-health-services" id="mental-health-services">
                        <h2>Mental and behavioral health</h2>
                        {% for mentalHealthService in mentalHealthServices.entities %}
                            {% include "src/site/facilities/health_service.drupal.liquid" with healthService = mentalHealthService %}
                        {% endfor %}
                    </section>
                {% endif %}

                {% if specialtyCareHealthServices.entities.length %}
                    <section class="specialty-health-services" id="specialty-health-services">
                        <h2>Specialty care</h2>
                        {% for specialtyService in specialtyCareHealthServices.entities %}
                            {% include "src/site/facilities/health_service.drupal.liquid" with healthService = specialtyService %}
                        {% endfor %}
                    </section>
                {% endif %}

                {% if veteranCareHealthServices.entities.length %}
                    <section class="veteran-care-health-services" id="veteran-care-health-services">
                        <h2>Services for Veteran care</h2>
                        {% for veteranService in veteranCareHealthServices.entities %}
                            {% include "src/site/facilities/health_service.drupal.liquid" with healthService = veteranService %}
                        {% endfor %}
                    </section>
                {% endif %}

                {% if otherHealthServices.entities.length %}
                    <section class="veteran-care-health-services" id="other-health-services">
                        <h2>Other services</h2>
                        {% for otherService in otherHealthServices.entities %}
                            {% include "src/site/facilities/health_service.drupal.liquid" with healthService = otherService %}
                        {% endfor %}
                    </section>
                {% endif %}

            </article>
        </div>
    </div>
  </main>
</div>
{% include "src/site/includes/footer.html" %}
{% include "src/site/includes/debug.drupal.liquid" %}
