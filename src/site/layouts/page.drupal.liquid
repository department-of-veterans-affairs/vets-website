{% include "src/site/includes/header.html" %}
<main>
  <div class="row">
    <article class="usa-content">
      <h1>{{ title }}</h1>
      <div class="usa-font-lead">{{ fieldIntroText }}</div>
      {% for block in fieldContentBlock %}
        {{ block.entity.entityRendered }}
      {% endfor %}
    </article>
    <div class="last-updated usa-content">
        Last updated: <time datetime="{{ fieldLastUpdate.value }}">{{ fieldLastUpdate.value }}</time>
    </div>
  </div>
</main>
