const { getDrupalValue } = require('./helpers');

const transform = entity => ({
  entity: {
    entityType: '<%= entityType %>',
    entityBundle: '<%= entityBundle %>',
    <%_ propertyNames.forEach(key => { _%>
    <%= key _%>: getDrupalValue(entity.<%= key %>),
    <%_ }); _%>
  },
});
module.exports = transform;
