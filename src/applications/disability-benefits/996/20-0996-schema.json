{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Higher-Level Review (20-0996)",
  "type": "object",
  "definitions": {
    "formType": {
      "type": "string",
      "enum": [
        "HigherLevelReview"
      ]
    },
    "benefitType": {
      "type": "string",
      "enum": [
        "compensation"
      ],
      "enumNames": [
        "Compensation"
      ]
    },
    "phoneCountryCode": {
      "type": "string",
      "pattern": "^[0-9]+$"
    },
    "phoneAreaCode": {
      "type": "string",
      "pattern": "^[2-9][0-9]{2}$"
    },
    "phoneNumber": {
      "type": "string",
      "pattern": "^[0-9]{1,14}$"
    },
    "phoneExt": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]{1,10}$"
    },
    "zipCode5": {
      "type": "string",
      "pattern": "^[0-9]{5}$"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "timeZone": {
      "type": "string"
    }
  },
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "type": {
          "$ref": "#/definitions/formType"
        },
        "attributes": {
          "type": "object",
          "required": [
            "benefitType",
            "veteran",
            "sameOffice",
            "informalConference"
          ],
          "benefitType": {
            "$ref": "#/definitions/benefitType"
          },
          "veteran": {
            "type": "object",
            "required": [
              "address"
            ],
            "properties": {
              "address": {
                "required": [
                  "zipCode5"
                ],
                "properties": {
                  "zipCode5": {
                    "$ref": "#/definitions/zipCode5"
                  }
                }
              },
              "phone": {
                "type": "object",
                "properties": {
                  "areaCode": {
                    "$ref": "#/definitions/phoneAreaCode"
                  },
                  "phoneNumber": {
                    "$ref": "#/definitions/phoneNumber"
                  },
                  "phoneNumberCountryCode": {
                    "$ref": "#/definitions/phoneCountryCode"
                  },
                  "phoneNumberExt": {
                    "$ref": "#/definitions/phoneExt"
                  }
                }
              },
              "emailAddressText": {
                "$ref": "#/definitions/email"
              },
              "timeZone": {
                "$ref": "#/definitions/timeZone"
              }
            }
          },
          "sameOffice": {
            "type": "boolean"
          },
          "informalConference": {
            "type": "boolean",
            "enum": [
              true
            ]
          },
          "informalConferenceRep": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "phone": {
                "type": "object",
                "properties": {
                  "areaCode": {
                    "$ref": "#/definitions/phoneAreaCode"
                  },
                  "phoneNumber": {
                    "$ref": "#/definitions/phoneNumber"
                  },
                  "phoneNumberCountryCode": {
                    "$ref": "#/definitions/phoneCountryCode"
                  },
                  "phoneNumberExt": {
                    "$ref": "#/definitions/phoneExt"
                  }
                }
              }
            }
          },
          "informalConferenceTimes": {
            "type": "array",
            "minItems": 1,
            "maxItems": 2,
            "uniqueItems": true,
            "items": {
              "type": "string",
              "enum": [
                "800-1000 ET",
                "1000-1230 ET",
                "1230-1400 ET",
                "1400-1630 ET"
              ]
            }
          }
        }
      }
    },
    "included": {
      "type": "array",
      "minItems": 1,
      "maxItems": 100,
      "items": {
        "type": "object",
        "required": [
          "type"
        ],
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "ContestableIssue"
            ]
          },
          "attributes": {
            "type": "object",
            "required": [
              "issue",
              "decisionDate"
            ],
            "properties": {
              "issue": {
                "type": "string",
                "maxLength": 140
              },
              "decisionDate": {
                "type": "string",
                "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
              },
              "decisionIssueId": {
                "type": "number"
              },
              "ratingIssueId": {
                "type": "string"
              },
              "ratingDecisionIssueId": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
