{
  "tags": {
    "allowUnknownTags": [
      "component",
      "category",
      "subcategory"
    ]
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/component",
    "node_modules/better-docs/category"
  ],
  "source": {
    "include": "./src/applications/vaos",
    "includePattern": ".+.js(doc|x)?$"
  },
  "opts": {
    "template": "node_modules/better-docs",
    "recurse": true,
    "destination": "./src/applications/vaos/docs/",
    "readme": "./src/applications/vaos/README.md"
  },
  "docdash": {
    "sort": true,
    "collapse": true,
    "typedefs": true,
    "menu": {
      "Product documentation": {
        "href": "https://github.com/department-of-veterans-affairs/va.gov-team/tree/master/products/health-care/appointments/va-online-scheduling"
      },
      "Staging": {
        "href": "https://staging.va.gov/health-care/schedule-view-va-appointments/appointments/"
      },
      "Local instance": {
        "href": "http://localhost:3001/health-care/schedule-view-va-appointments/appointments/"
      }
    }
  }
}
