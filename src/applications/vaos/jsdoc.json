{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": "./src/applications/vaos",
    "includePattern": ".+.js(doc|x)?$"
  },
  "opts": {
    "template": "node_modules/docdash",
    "recurse": true,
    "destination": "./src/applications/vaos/docs/"
  },
  "docdash": {
    "sort": true,
    "collapse": true,
    "typedefs": true
  }
}
