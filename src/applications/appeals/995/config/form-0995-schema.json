        "phoneNumber": {
          "type": "string",
          "pattern": "^[0-9]{1,14}$",
          "minLength": 1,
          "maxLength": 14
        },
        "phoneNumberExt": {
          "type": "string",
          "pattern": "^[a-zA-Z0-9]{1,10}$",
          "minLength": 1,
          "maxLength": 10
