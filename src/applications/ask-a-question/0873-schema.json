        "suffix": {
          "type": "string",
          "enum": [
            "Jr.",
            "Sr.",
            "II",
            "III",
            "IV"
          ]
        }
      },
      "required": [
        "first",
        "last"
      ]
    },
    "preferredContactMethod": {
      "default": "email",
      "type": "string",
      "enum": [
        "email",
        "phone",
        "mail"
      ],
      "enumNames": [
        "Email",
        "Phone",
        "US Mail"
      ]
    },
