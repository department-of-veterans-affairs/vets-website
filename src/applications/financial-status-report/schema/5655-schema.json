{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Financial Status Report 5655",
  "type": "object",
  "definitions": {
    "fullName": {
      "type": "object",
      "properties": {
        "first": {
          "type": "string",
          "ui:title": "First name",
          "minLength": 1,
          "maxLength": 30
        },
        "middle": {
          "type": "string",
          "ui:title": "Middle name"
        },
        "last": {
          "type": "string",
          "ui:title": "Last name",
          "minLength": 1,
          "maxLength": 30
        },
        "suffix": {
          "type": "string",
          "ui:title": "Middle name",
          "enum": ["Jr.", "Sr.", "II", "III", "IV"]
        }
      },
      "required": ["first", "last"]
    }
  },
  "properties": {
    "veteranFullName": {
      "$ref": "#/definitions/fullName"
    },
    "veteranSocialSecurityNumber": {
      "type": "string"
    },
    "veteranDateOfBirth": {
      "type": "string"
    },
    "vaFileNumber": {
      "type": "string",
      "pattern": "^[cC]{0,1}\\d{7,9}$"
    }
  }
}
