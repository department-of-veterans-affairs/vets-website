  "lint-staged": {
    "*.(js|jsx)": "eslint --fix"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/platform/testing/**",
      "**/*.spec.js",
      "**/*.spec.jsx"
    ]
