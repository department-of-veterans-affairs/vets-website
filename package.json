{
  "name": "vets-website",
  "version": "1.0.0",
  "description": "Frontend build for vets.gov website static assets.",
  "scripts": {
    "analyze": "webpack-bundle-analyzer build/production/generated/stats.json",
    "build": "node script/build.js",
    "build-analyze": "NODE_ENV=production npm run build -- --buildtype production --analyzer; npm run analyze",
    "prebuild": "node script/prebuild.js",
    "heroku-serve": "http-server",
    "heroku-postbuild": "npm run build --  --entry no-react,style",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:changed": "npm run lint:js:changed && npm run lint:sass",
    "lint:js": "eslint --quiet --ext .js --ext .jsx .",
    "lint:js:changed": "LIST=`git diff-index --name-only HEAD | grep .*\\.js | grep -v json`; if [ \"$LIST\" ]; then eslint --quiet $LIST; fi",
    "lint:sass": "sass-lint -c config/sass-lint.yml --verbose",
    "nightwatch:docker": "nightwatch -c config/nightwatch.docker-compose.js --retries 3",
    "nightwatch": "nightwatch -c config/nightwatch.js",
    "nightwatch-sauce": "nightwatch -c config/nightwatch-sauce.js",
    "nightwatch-visual": "nightwatch test/visual-regression/index.js -c config/nightwatch.js",
    "postinstall": "npm rebuild node-sass",
    "reset:env": "./script/reset-environment.sh",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "./script/run-nightwatch.sh",
    "test:e2e:docker": "docker-compose -p e2e up -d && docker-compose -p e2e run --user=node --rm --entrypoint=npm -e BABEL_ENV=test -e BUILDTYPE=production vets-website --no-color run nightwatch:docker && docker-compose -p e2e stop",
    "test:accessibility": "./script/run-nightwatch.sh --env accessibility",
    "test:accessibility:docker": "docker-compose -p accessibility up -d && docker-compose -p accessibility run --rm --entrypoint=npm -e BABEL_ENV=test -e BUILDTYPE=production vets-website --no-color run nightwatch:docker -- --env=accessibility && docker-compose -p accessibility stop",
    "test:best-practice": "./script/run-nightwatch.sh --env bestpractice",
    "test:coverage": "NODE_ENV=test nyc --all --reporter=lcov --reporter=text --reporter=html mocha --no-color --recursive 'test/**/*.unit.spec.js?(x)' test/helper.js",
    "test:sauce": "./script/run-nightwatch.sh --sauce",
    "test:sauce:desktop": "./script/run-nightwatch.sh --sauce -e chrome,firefox,ie,edge",
    "test:sauce:mobile": "./script/run-nightwatch.sh --sauce -e ios,android",
    "test:unit": "./script/run-unit-test.sh",
    "test:unit:edu": "BABEL_ENV=test mocha test/helper.js --recursive 'test/edu-benefits/**/*.unit.spec.js?(x)' ",
    "test:unit:hca": "BABEL_ENV=test mocha test/helper.js --recursive 'test/hca/**/*.unit.spec.js?(x)' ",
    "test:unit:msg": "BABEL_ENV=test mocha test/helper.js --recursive 'test/messaging/**/*.unit.spec.js?(x)' ",
    "test:unit:hr": "BABEL_ENV=test mocha test/helper.js --recursive 'test/health-records/**/*.unit.spec.js?(x)' ",
    "test:unit:rx": "BABEL_ENV=test mocha test/helper.js --recursive 'test/rx/**/*.unit.spec.js?(x)' ",
    "test:unit:gi": "BABEL_ENV=test mocha test/helper.js --recursive 'test/gibct/**/*.unit.spec.js?(x)' ",
    "test:unit:login": "BABEL_ENV=test mocha --recursive 'test/login/**/*.unit.spec.js?(x)' ",
    "test:unit:db": "BABEL_ENV=test mocha --recursive 'test/disability-benefits/**/*.unit.spec.js?(x)' ",
    "test:watch": "BABEL_ENV=test mocha --watch --recursive 'test/**/*.unit.spec.js?(x)' ",
    "test:visual": "./script/run-nightwatch.sh --visual",
    "test:visual:baseline": "./script/run-nightwatch.sh --visual --command=baseline",
    "watch": "node script/build.js --watch",
    "watch:static": "npm run watch -- --entry no-react"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/department-of-veterans-affairs/vets-website.git"
  },
  "bugs": {
    "url": "https://github.com/department-of-veterans-affairs/vets-website/issues"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**"
    ]
  },
  "homepage": "https://github.com/department-of-veterans-affairs/vets-website#readme",
