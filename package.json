{
  "name": "vets-website",
  "version": "1.0.0",
  "description": "Frontend build for vets.gov website static assets.",
  "scripts": {
    "build": "node script/build.js",
    "heroku-serve": "http-server",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "eslint --ignore-pattern src/js/legacy --ext .js --ext .jsx src/js test/js",
    "lint:sass": "sass-lint -c config/sass-lint.yml",
    "selenium:bootstrap": "selenium-standalone install",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "nightwatch -c config/nightwatch.js",
    "test:unit": "mocha --recursive --compilers js:babel-register,jsx:babel-register --prof 'test/**/*.unit.spec.js?(x)' ",
    "watch": "AWONG_TEST=watch node script/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/department-of-veterans-affairs/vets-website.git"
  },
  "bugs": {
    "url": "https://github.com/department-of-veterans-affairs/vets-website/issues"
  },
  "homepage": "https://github.com/department-of-veterans-affairs/vets-website#readme",
