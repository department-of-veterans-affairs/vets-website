{
  "name": "eslint-plugin-va",
  "version": "1.0.3",
  "main": "index.js",
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint . --cache",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-plugin": "^4.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "requireindex": "^1.2.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testMatch": [
      "**/tests/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/tests/helpers/"
    ]
  }
}
