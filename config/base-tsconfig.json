{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "lib": ["es2020"],
    "target": "es2020",
    "jsx": "react"
  },
  "exclude": ["node_modules"]
}
